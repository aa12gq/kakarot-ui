{
  "extends": "@vue/tsconfig/tsconfig.web.json",
  "include": ["env.d.ts", "src/*.vue", "src/**/*", "src/**/*.vue", "src/auto-import.d.ts", "page/main.ts"],
  "exclude": ["src/**/__tests__/*"],
  "compilerOptions": {
    "composite": true,
    "baseUrl": ".",
    "allowJs": true,
    "importsNotUsedAsValues": "preserve",
    "isolatedModules": false,
    "paths": {
      "@/*": ["./src/*"]
    }
  }
}
